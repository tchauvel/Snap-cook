{
  "expo": {
    "name": "Snap & Cook",
    "slug": "snap-and-cook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/snapcook.png",
    "scheme": "snap-and-cook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/snapcook.png",
      "resizeMode": "contain",
      "backgroundColor": "#E07A5F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.snapandcook.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/snapcook.png",
        "backgroundColor": "#E07A5F"
      },
      "package": "com.snapandcook.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/snapcook.png"
    },
    "plugins": [
      "expo-camera",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "testMode": true
    }
  }
}